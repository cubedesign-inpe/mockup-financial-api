{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "ecma5",
    "ecma6",
    "chai"
  ],
  "loadEagerly": [
    "app/**/*.js",
    "test/**/*.js",
    "config/**/*.js",
    "start/**/*.js",
    "database/**/*.js"
  ],
  "dontLoad": [
    "node_modules/**"
  ],
  "plugins": {
    "modules": {},
    "es_modules": {},
    "node": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }
  }
}
